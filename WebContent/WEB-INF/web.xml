<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>finalTest</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
  	<servlet-name>Front</servlet-name>
  	<servlet-class>finalTest.controller.FrontController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Front</servlet-name>
  	<url-pattern>*.do</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>Professor</servlet-name>
  	<servlet-class>finalTest.controller.ProfController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Professor</servlet-name>
  	<url-pattern>*.prof</url-pattern>
  </servlet-mapping>
  
   <servlet>
  	<servlet-name>Student</servlet-name>
  	<servlet-class>finalTest.controller.StudentController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Student</servlet-name>
  	<url-pattern>*.stu</url-pattern>
  </servlet-mapping>
</web-app>